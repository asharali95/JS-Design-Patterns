    "startSingleton": "yarn run singletonPatternTranspile && node ./output/singletonIndex.js"
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
